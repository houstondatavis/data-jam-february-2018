CREATE TABLE census.alldata (
	series_id       text,
	year            smallint,
	period          text,
	value           text,
	footnote_codes  text
);

CREATE INDEX ON census.alldata ( census.parse_series_id(series_id) );

\COPY census.alldata FROM './data/raw_data/cx.data.1.AllData.txt' WITH ( FORMAT CSV, HEADER true, DELIMITER E'\t' );
